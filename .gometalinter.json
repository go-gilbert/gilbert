{
  "Vendor": true,
  "Test": true,
  "Skip": ["mock"],
  "Exclude": [".+_gen\\.go"],
  "Deadline": "1500s",
  "Sort": ["linter", "severity"],
  "Linters": {
    "errcheck":{
      "Command":"errcheck -ignoretests"
    }
  },
  "Enable": [
    "deadcode",
    "errcheck",
    "goconst",
    "gocyclo",
    "gofmt",
    "golint",
    "ineffassign",
    "interfacer",
    "maligned",
    "structcheck",
    "unconvert",
    "varcheck",
    "vet"
  ]
}
